{
  "name": "aoi",
  "version": "0.0.2",
  "description": "a ReactJS isomorphic framework",
  "main": "index.js",
  "author": "yussan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yussan/react-redux-isomorphic-boilerplate/issues"
  },
  "homepage": "https://github.com/yussan/react-redux-isomorphic-boilerplate#readme",
  "scripts": {
    "start": "nodemon --watch src/ --watch assets/ --exec 'node -r dotenv/config -r babel-register' ./src/server/index",
    "webpack": "rimraf assets/build && NODE_ENV=development webpack --watch --progress",
"prepublish": "npm run prod:webpack",    
"prod:webpack": "rimraf ./assets/build && NODE_ENV=production webpack --progress",
    "prod:server": "rimraf ./dist && NODE_ENV=production babel src -d dist --ignore '**/*_test.js' --copy-files",
    "prod:build": "npm run prod:webpack && npm run prod:server",
    "prod:test-start": "NODE_ENV=production nodemon --watch src/ --watch assets/ --exec 'node -r dotenv/config' ./dist/server/index",
    "release": "standard-version",
    "release:beta": "standard-version -p beta"
  },
  "dependencies": {
    "express": "^4.15.4",
    "formidable": "^1.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "string-manager": "^1.1.2",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "compression": "^1.7.0",
    "dotenv": "^4.0.0",
    "express-debug": "^1.1.1",
    "nodemon": "^1.12.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "webpack": "^3.5.6"
  }
}
